% Generated by roxygen2: do not edit by hand
% Please edit documentation in R/plot_multiplex.R
\name{plot_multiplex}
\alias{plot_multiplex}
\title{plot general multiplex network function (similar to temporal and spatial)}
\usage{
plot_multiplex(
  networks,
  interlayer,
  binary = FALSE,
  y_adjust = 1,
  y_adjust_h = 1.5,
  gap = 0.6,
  label = FALSE,
  extend = NULL,
  inter_lty = 2,
  inter_lcol = "black",
  intra_lty = 1,
  intra_lcol = "black",
  inter_lsize = 1,
  intra_lsize = 1,
  node_size = 1,
  label_size = 1.5,
  alpha = 0.2,
  ...
)
}
\arguments{
\item{networks}{network list}

\item{interlayer}{a data frame with `layer_from`,`node_from`,`layer_to`, `node_to`, and `weight` columns.}

\item{binary}{TRUE or FALSE: return a binary or weighted multiplex network}

\item{y_adjust}{control the shape of polygon}

\item{y_adjust_h}{control the shape of polygon}

\item{gap}{overlap between layers}

\item{label}{TRUE or FALSE: nodes with label or not, default is FALSE}

\item{extend}{extend information about nodes}

\item{inter_lty}{line type of inter-links}

\item{inter_lcol}{line color of inter-links}

\item{intra_lty}{line type of intra-links}

\item{intra_lcol}{line color of intra-links}

\item{inter_lsize}{line size of inter-links}

\item{intra_lsize}{line size of intra-links}

\item{node_size}{node size}

\item{label_size}{label size}

\item{alpha}{transparency of layers}

\item{...}{}
}
\value{
ggplot2 object
}
\description{
plot general multiplex network function (similar to temporal and spatial)
}
\examples{
multiplex_net <- list()
for (i in 1:3) {
  multiplex_net[[i]] <- empty(time_nets[[i]])
  }
plot_multiplex(networks = multiplex_net, interlayer = interlayer,
               binary = T,node_size = 4, intra_lcol = "#00AFBB", label = T,
              inter_lcol = "#E7B800", inter_lty = 5, y_adjust = 2) +
               scale_color_manual(values = c("#00AFBB", "#E7B800")) +
               scale_fill_manual(values = c("#00AFBB", "#E7B800", "#FC4E07"))


}
